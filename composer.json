{
    "name": "deshithemes/css-cleaner",
    "description": "ðŸš€ Laravel CSS Optimizer: Purge unused CSS, Minify styles, Boost website speed & SEO. Zero-config setup for Tailwind, Bootstrap. 30%+ smaller CSS files guaranteed.",
    "version": "1.1.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel-css-optimizer",
        "purge-unused-css",
        "css-minifier",
        "website-speed",
        "seo-tool",
        "tailwind-optimizer",
        "bootstrap-optimizer",
        "frontend-optimization",
        "performance-tool"
    ],
    "authors": [
        {
            "name": "Ruman (DeshiThemes)",
            "email": "roman.civil2019@gmail.com",
            "homepage": "https://github.com/DeshiThemes"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "illuminate/support": "^10.0 || ^11.0",
        "symfony/process": "^6.0 || ^7.0",
        "ext-json": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "DeshiThemes\\CssCleaner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeshiThemes\\CssCleaner\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DeshiThemes\\CssCleaner\\CssCleanerServiceProvider"
            ],
            "aliases": {
                "CssCleaner": "DeshiThemes\\CssCleaner\\Facades\\CssCleaner"
            }
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=css-cleaner-config --ansi"
        ],
        "test": [
            "@phpunit"
        ]
    },
    "suggest": {
        "node": ">=16.0.0 required for PurgeCSS functionality",
        "postcss": "Required for advanced CSS processing"
    }
}